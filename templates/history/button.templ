package history

templ HistoryButton() {
	<button
		id="history-ticker-section"
		hx-get="/history/button"
		hx-trigger="click"
		hx-target="#dashboard"
		hx-swap="innerHTML"
		type="button"
		class="bg-white hover:opacity-50 p-3 rounded-2xl border border-black border-solid  max-w-1xl"
	>History</button>
}

{
  "devDependencies": {
    "@tailwindcmd/static/css/forms": "^0.5.9",
    "@tailwindcmd/static/css/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "flowbite": "^2.5.2",
    "simple-datatables": "^9.2.1"
  },
  "name": "trends",
  "version": "1.0.0",
  "description": "Go based app to visualize market trends.",
  "main": "postcss.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "css": "tailwindcss -i cmd/static/css/input.css -o cmd/static/css/output.css --minify",
    "css:watch": "tailwindcss -i cmd/static/css/input.css -o cmd/static/css/output.css --watch",
    "css:build": "tailwindcss build cmd/static/css/input.css -o cmd/static/css/output.css",
    "postcss": "postcss cmd/static/css/output.css --use postcss-import --dir cmd/static/css/dist",
    "postcss:watch": "postcss cmd/static/css/output.css --use postcss-import --dir cmd/static/css/dist --watch"
  },
